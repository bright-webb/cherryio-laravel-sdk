{
  "name": "cherryio/laravel",
    "description": "Cherryio broadcaster for Laravel",
    "type": "library",
    "version": "1.0.0",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Cherryio\\": "src/"
        }
    },
    "require": {
        "bright-webb/cherryio-php-sdk": "^1.0"
    },
    
    "extra": {
